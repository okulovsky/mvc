@model IEnumerable<Department>
 
@{
    ViewBag.Title = "Departments";
    ViewBag.Header = "Departments summary";
    Layout = "~/Views/Shared/Layout06.cshtml";
}

@section Body
{
    @foreach (var dep in Model)
    {
        Html.RenderPartial("PartialDep", dep);
    }
}

@section Footer
{
@Html.Today2()
}
